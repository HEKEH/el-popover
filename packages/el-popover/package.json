{
  "name": "@hekeh/el-popover",
  "version": "0.1.0",
  "description": "A popover component extracted from element-plus",
  "keywords": [
    "Element",
    "Element Plus",
    "vue",
    "popover",
    "tooltip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HEKEH/el-popover"
  },
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vue-tsc --declaration --noEmit && vite build --watch",
    "build": "vue-tsc --declaration --noEmit && vite build",
    "lint": "eslint",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@el-popover/theme-chalk": "workspace:*",
    "@floating-ui/dom": "^1.0.1",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vueuse/core": "^9.1.0",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
